<% flash.each do |key, value| %>
  <div class="<%= flash_class(key) %> alert-dismissible fade show" role="alert">
    <button type="button" class="close" data-dismiss="alert">
      <span aria-hidden="true">&times;</span>
    </button>
    <strong>
      <%= value %>
    </strong>
  </div>
<% end %>
